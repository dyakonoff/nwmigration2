<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.company.nwtest2.web.orderdetailsstatus.OrderDetailsStatusEdit"
        datasource="orderDetailsStatusDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.nwtest2.web.orderdetailsstatus">
    <dsContext>
        <datasource id="orderDetailsStatusDs"
                    class="com.company.nwtest2.entity.OrderDetailsStatus"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="orderDetailsStatusDs">
            <column width="250px">
                <field property="status_Name"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
